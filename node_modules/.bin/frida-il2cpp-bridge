#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -x "$basedir/python3" ]; then
  exec "$basedir/python3"  "$basedir/../frida-il2cpp-bridge/cli/main.py" "$@"
else 
  exec python3  "$basedir/../frida-il2cpp-bridge/cli/main.py" "$@"
fi
